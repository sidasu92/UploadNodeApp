<!DOCTYPE html>
<html>
<head>
  <meta charset="UTF-8">
  <script src="http://ajax.googleapis.com/ajax/libs/jquery/1.11.1/jquery.min.js"></script>
</head>

<body>
  <div class="mui-container">
    <h1>Here is file descriptions</h1>
    <form>
      <table class="mui-table mui-table--bordered">
        <thead>
          <tr>
            <th>New Filename</th>
            <th>Size</th>
            <th>MD5 checksum of file</th>
            <th>Upload at</th>
            <th><i class="fa fa-download"></i>Download</th>
            <th>Share This file with: </th>
          </tr>
        </thead>
        <tbody>
          
          <% for (var i = 0; i < myArray.length; i++) { 
              
              
          } %>
          <tr>
            <td><%= file.filename %></td>
            <td><%= file.length %></td>
            <td><%= file.md5 %></td>
            <td><%= file.uploadDate %></td>
            <td><a href="/files/<%= file.filename %>">Download me!!</a></td>
            <td><input id="myInput" type="text" name="username"><a href="/share/<%= file.filename %>" id="shareLink">Share</a></td>
          </tr>
        </tbody>
      </table>
    </form>
    <button class="mui-btn mui-btn--raised mui-btn--primary" onclick="window.location='/';">
      Go back
    </button>

  </div>
</body>
<script type="text/javascript">
  $(function() {
    $("#shareLink").click(function() {
      window.location = $(this).attr('href') + '/' + $('#myInput').val();
      return false;
    });
  })
</script>>
</html>
